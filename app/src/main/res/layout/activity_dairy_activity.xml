<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/thm2"
    android:orientation="vertical"
    tools:context=".dairy.dairy_activity"
    app:layout_constraintStart_toStartOf="parent">

    <!--    #첫 번째 줄-->
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="*"
        android:layout_marginBottom="30dp"
        tools:context=".TableMainActivity">

        <TableRow>

            <ImageButton
                android:layout_width="60dp"
                android:layout_height="50dp"
                android:layout_column="0"
                android:onClick="onClick"
                tools:srcCompat="@drawable/amazing_256"/>


            <ImageButton
                android:layout_width="60dp"
                android:layout_height="50dp"
                android:layout_column="6"
                tools:srcCompat="@drawable/expect_256"/>

        </TableRow>

    </TableLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="480dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="120dp"
            tools:context=".TableMainActivity"></LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">




            <!--달력뷰-->

            <CalendarView
                android:id="@+id/calendarView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="vertical">

        <ImageView
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            tools:srcCompat="@drawable/amazing_256"></ImageView>

    </LinearLayout>


</LinearLayout>
