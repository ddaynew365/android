    package="coms.example.allinonelowfiproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".dairy.dairy_list"></activity>
        <activity android:name=".dairy.dairy_write" />
        <activity android:name=".dairy.dairy_emotion" />
        <activity android:name=".dairy.dairy_activity" />
        <activity android:name=".HouseKeepingBook" />
        <activity android:name=".BeforeAndAfter" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name=".mainCalendar.MaterialCalendarViewInitProvider"
            android:authorities="calendar" />
    </application>

</manifest>